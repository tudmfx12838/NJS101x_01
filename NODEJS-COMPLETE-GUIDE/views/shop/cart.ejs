<%- include('../includes/head.ejs') %>
    </head>

    <body>
        <%- include('../includes/navigation.ejs') %>
            <main>
                <% if (products.length > 0) { %>
                    <ul>
                        <% products.forEach(prod => { %>
                            <li><%= prod.productData.title %> (<%= prod.qty %>)</li>
                        <% }); %>
                    </ul>
                <% } else { %>
                    <h1>No Product In Cart!!!</h1>
                <% } %>
            </main>
        <%- include('../includes/end.ejs') %>